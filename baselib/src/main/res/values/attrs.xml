<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="exAnimDuration" format="integer"/>
        <attr name="exAnimAlphaStart" format="float"/>
        <attr name="expandDrawable" format="reference"/>
        <attr name="collapseDrawable" format="reference"/>
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="v_border_width" format="dimension"/>
        <attr name="v_border_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="RoundTextView">
        <attr name="radius" format="dimension"/>
        <attr name="backgroundColor" format="color"/>
    </declare-styleable>
</resources>